#!/bin/sh

echo "Preparing Revolutionist76..."
echo "Installing Tor Browser"
curl https://gist.githubusercontent.com/kylerschin/dcba27d5ab849ef345e3e4af3f52b8df/raw/1008ad26498024465369a92e9d1d7ed7974a1678/gistfile1.txt | bash

echo "Transmission - Torrent"
sudo add-apt-repository ppa:transmissionbt/ppa
apt update
sudo apt-get install transmission-cli transmission-common transmission-daemon

echo "installing GPG"
sudo apt-get install gnupg

echo "Installing Bitcoin"
sudo add-apt-repository ppa:bitcoin/bitcoin
sudo apt update
sudo apt install bitcoin-qt bitcoind

echo "Installing Keyringer"
apt install keyringer

echo "
